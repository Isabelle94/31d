<!DOCTYPE html>
<html ng-app="mobilePost">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}MobilePost{% endblock %}</title>
        {% block stylesheets %}
			<link rel="stylesheet" href="{{ asset('bundles/app/css/style.css') }}"/>
		{% endblock %}        
		{% block javascripts %}
			<script src="{{ asset('bundles/app/node_modules/angular/angular.js') }}"></script>
			<script src="{{ asset('bundles/app/node_modules/angular-resource/angular-resource.js') }}"></script>
			<script src="{{ asset('bundles/app/node_modules/angular-route/angular-route.js') }}"></script>	  	
			<script src="{{ asset('bundles/app/js/app.js') }}"></script>
			<script src="{{ asset('bundles/app/js/services.js') }}"></script>
			<script src="{{ asset('bundles/app/js/controllers.js') }}"></script>
			<script src="{{ asset('bundles/app/js/directives.js') }}"></script>
		{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}			
			<div ng-view></div>
		{% endblock %}
    </body>
</html>
